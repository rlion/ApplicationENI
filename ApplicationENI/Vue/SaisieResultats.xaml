<UserControl x:Class="ApplicationENI.Vue.SaisieResultats"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="460" d:DesignWidth="630"
             xmlns:chartingToolkit="http://schemas.microsoft.com/wpf/2008/toolkit"
             >
    <!--Grid Loaded="Grid_Loaded"-->
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="140,11,0,0" Name="cbECF" VerticalAlignment="Top" Width="162" Text="" SelectionChanged="cbECF_SelectionChanged" />
        <Label Content="ECF :" Height="28" HorizontalAlignment="Left" Margin="35,11,0,0" Name="lbECF" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Right" />
        <ListBox Height="220" HorizontalAlignment="Left" Margin="35,190,0,0" Name="listBox1" VerticalAlignment="Top" Width="267" ItemsSource="{Binding}" SelectionMode="Single" IsEnabled="False" />
        <ListBox Height="220" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="330,190,0,0" Name="listBox2" SelectionMode="Single" VerticalAlignment="Top" Width="263" IsEnabled="False" />
        <ComboBox Height="23" Margin="140,47,0,0" Name="comboBox1" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="54" IsEnabled="False" />
        <Label Content="Version :" Height="28" HorizontalAlignment="Left" Margin="35,47,0,0" Name="label1" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Right" IsEnabled="False" />
        <Button Content="Enregistrer" Height="23" HorizontalAlignment="Left" Margin="407,425,0,0" Name="btnAjouter" VerticalAlignment="Top" Width="90" />
        <Button Content="Annuler" Height="23" HorizontalAlignment="Left" Margin="503,425,0,0" Name="btnModifier" VerticalAlignment="Top" Width="90" />
        <GroupBox Header="Note" Height="50" HorizontalAlignment="Left" Margin="35,134,0,0" Name="groupBox1" VerticalAlignment="Top" Width="273" IsEnabled="False">
            <Grid>
                <RadioButton Content="Acquis" Height="15" HorizontalAlignment="Left" Margin="6,5,0,0" Name="radioButton2" VerticalAlignment="Top" Width="64" Visibility="Hidden" />
                <RadioButton Content="En cours" Height="15" Margin="82,5,109,0" Name="radioButton3" VerticalAlignment="Top" HorizontalAlignment="Center" Visibility="Hidden" />
                <RadioButton Content="Non acquis" Height="15" HorizontalAlignment="Right" Margin="0,5,8,0" Name="radioButton4" VerticalAlignment="Top" Width="76" Visibility="Hidden" />
                <TextBox Height="20" HorizontalAlignment="Left" Margin="82,3,0,0" Name="textBox1" VerticalAlignment="Top" Width="163" Visibility="Hidden" />
                <Label Content="Note :" Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Right" Margin="-24,0,0,0" Name="label3" VerticalAlignment="Top" Width="86" Visibility="Hidden" />
            </Grid>
        </GroupBox>
        <Button Content="Planifier ECF" Height="23" HorizontalAlignment="Left" Margin="35,425,0,0" Name="btAdd1" VerticalAlignment="Top" Width="90" Click="btAdd1_Click" />
        <Button Content="Supprimer ECF" Height="23" HorizontalAlignment="Left" Margin="131,425,0,0" Name="btDelete" VerticalAlignment="Top" Width="90" />
        <Calendar Height="173" HorizontalAlignment="Left" Margin="403,11,0,0" Name="calendar1" VerticalAlignment="Top" Width="190" IsEnabled="False" MaxWidth="Infinity" SelectionMode="MultipleRange" />
    </Grid>
</UserControl>
