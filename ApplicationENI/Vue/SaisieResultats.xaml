<UserControl x:Class="ApplicationENI.Vue.SaisieResultats"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="460" d:DesignWidth="630">
    <Grid Loaded="Grid_Loaded">
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="140,11,0,0" Name="cbECF" VerticalAlignment="Top" Width="162" Text="" />
        <Label Content="ECF :" Height="28" HorizontalAlignment="Left" Margin="35,11,0,0" Name="lbECF" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Right" />
        <ListBox Height="283" HorizontalAlignment="Left" Margin="35,90,0,0" Name="listBox1" VerticalAlignment="Top" Width="267" ItemsSource="{Binding}" SelectionMode="Single" />
        <ListBox Height="283" HorizontalAlignment="Left" ItemsSource="{Binding}" Margin="330,90,0,0" Name="listBox2" SelectionMode="Single" VerticalAlignment="Top" Width="263" />
        <ComboBox Height="23" Margin="140,47,0,0" Name="comboBox1" Text="" VerticalAlignment="Top" HorizontalAlignment="Left" Width="54" />
        <Label Content="Version :" Height="28" HorizontalAlignment="Left" Margin="35,47,0,0" Name="label1" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Right" />
        <DatePicker Height="23" HorizontalAlignment="Left" Margin="394,14,0,0" Name="datePicker1" VerticalAlignment="Top" Width="170" SelectedDateFormat="Short" />
        <Label Content="Date" Height="28" HorizontalAlignment="Left" Margin="355,12,0,0" Name="label2" VerticalAlignment="Top" />
        <Button Content="Enregistrer" Height="23" HorizontalAlignment="Left" Margin="437,425,0,0" Name="btnAjouter" VerticalAlignment="Top" Width="75" />
        <Button Content="Annuler" Height="23" HorizontalAlignment="Left" Margin="518,425,0,0" Name="btnModifier" VerticalAlignment="Top" Width="75" />
        <GroupBox Header="Note" Height="41" HorizontalAlignment="Left" Margin="330,43,0,0" Name="groupBox1" VerticalAlignment="Top" Width="263">
            <Grid>
                <RadioButton Content="Acquis" Height="15" HorizontalAlignment="Left" Margin="10,3,0,0" Name="radioButton2" VerticalAlignment="Top" Width="64" />
                <RadioButton Content="En cours" Height="15" Margin="85,3,102,0" Name="radioButton3" VerticalAlignment="Top" HorizontalAlignment="Center" />
                <RadioButton Content="Non acquis" Height="15" HorizontalAlignment="Right" Margin="169,3,0,0" Name="radioButton4" VerticalAlignment="Top" Width="76" />
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
