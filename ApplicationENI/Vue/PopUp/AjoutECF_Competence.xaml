<Window x:Class="ApplicationENI.Vue.PopUp.AjoutECF_Competence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AjoutECF_Competence" Height="160" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Code : " Height="24" HorizontalAlignment="Left" Margin="22,13,0,0" Name="lbCode" VerticalAlignment="Top" Width="70" IsEnabled="True" HorizontalContentAlignment="Right" />
        <TextBox Height="24" HorizontalAlignment="Left" Margin="100,13,0,0" Name="tbCode" VerticalAlignment="Top" IsEnabled="True" Width="100" />
        <Label Content="Libellé : " Height="24" HorizontalAlignment="Left" Margin="22,49,0,0" Name="lbLibelle" VerticalAlignment="Top" Width="70" IsEnabled="True" HorizontalContentAlignment="Right" />
        <TextBox Height="24" HorizontalAlignment="Left" Margin="100,49,0,0" Name="tbLibelle" VerticalAlignment="Top" IsEnabled="True" Width="230" />
        <Button Content="Valider" Height="23" HorizontalAlignment="Left" Margin="168,86,0,0" Name="btValider" VerticalAlignment="Top" Width="75" Click="btValider_Click" IsDefault="True" />
        <Button Content="Annuler" Height="23" Margin="255,86,0,0" Name="btAnnuler" VerticalAlignment="Top" Click="btAnnuler_Click" IsCancel="True" HorizontalAlignment="Left" Width="75" />
    </Grid>
</Window>
